# Example of loading a CSV file from GitHub
url <- "https://raw.githubusercontent.com/friansakoko/Machine-Learning-with-R-Fourth-Edition/refs/heads/main/Chapter%2002/pt_data.csv"
data <- read.csv(url)

# View the first few rows of the data
head(data)

# Calculate the average temperature
mean_temp <- mean(data$temperature)
mean_temp

# Count the number of male and female subjects
gender_count <- table(data$gender)
gender_count

# Count the number of subjects by blood type
blood_type_count <- table(data$blood_type)
blood_type_count
